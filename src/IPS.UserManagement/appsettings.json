{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Duende": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "IdentityServer": "Data Source=IdentityServer.db;Cache=Shared",
    "UserManagement": "Data Source=UserManagement.db;Cache=Shared",
    "AspNetCoreIdentity": "Data Source=AspNetCoreIdentity.db;Cache=Shared"
  },
  "AllowedHosts": "*",
  "IdentityServer": {
    "Enabled": true
  }
}
